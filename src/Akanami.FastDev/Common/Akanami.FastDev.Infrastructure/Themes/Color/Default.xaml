<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Akanami.FastDev.Infrastructure.Themes.Color">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary>
            <!--矢量图标颜色-->
            <SolidColorBrush x:Key="COLOR_ICON">#FFFFFF</SolidColorBrush>
            <!--TOP背景颜色(深黑)-->
            <SolidColorBrush x:Key="COLOR_TOP_BACKGROUND">#303841</SolidColorBrush>
            <!--Body主颜色(浅黑)-->
            <SolidColorBrush x:Key="COLOR_BODY_BACKGROUND">#3a4750</SolidColorBrush>
            <!--分隔符(红)-->
            <SolidColorBrush x:Key="COLOR_SPLITER">#d72323</SolidColorBrush>
            <!--亮色-->
            <SolidColorBrush x:Key="COLOR_FONT">#eeeeee</SolidColorBrush>
            <!--标题颜色-->
            <SolidColorBrush x:Key="COLOR_TITLE">#FFFFFF</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>